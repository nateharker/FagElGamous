@model BioSampleListViewModel

@{
    ViewData["Title"] = "Bio Sample Data";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Bio Sample Data</h1>
<partial name="DbNavPartial" />
<br />
<partial name="BurialLocSearch" />
<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Burial Location ID
            </th>
            <th>
                Rack Number
            </th>
            <th>
                Bag Number
            </th>
            <th>
                Burial Item Number
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.BioSampleDatas)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.BurialLocNs) @Html.DisplayFor(modelItem => item.NsLow)/@Html.DisplayFor(modelItem => item.NsHigh) @Html.DisplayFor(modelItem => item.BurialLocEw) @Html.DisplayFor(modelItem => item.EwLow)/@Html.DisplayFor(modelItem => item.EwHigh) @Html.DisplayFor(modelItem => item.Subplot) #@Html.DisplayFor(modelItem => item.BurialNum)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.RackNum)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BagNum)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BurialItemId)
                </td>
                <td>
                    <a asp-action="Edit" asp-route-id="@item.BioSampleId">Edit</a> |
                    <a asp-action="Details" asp-route-id="@item.BioSampleId">Details</a> |
                    <a asp-action="Delete" asp-route-id="@item.BioSampleId">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>
<div class="text-center">
    <div page-info="@Model.PageNumberingInfo" page-classes-enabled="true"
         page-class="btn" page-class-normal="btn-outline-dark m-1" page-class-selected="btn-secondary m-1">
    </div>
</div>
